   "source": [
    "import modal\n",
    "\n",
    "app = modal.App(\"example-get-started\")\n",
    "\n",
    "\n",
    "@app.function()\n",
    "def square(x):\n",
    "    print(\"This code is running on a remote worker!\")\n",
    "    return x**2\n",
    "\n",
    "\n",
    "@app.local_entrypoint()\n",
    "def main():\n",
    "    print(\"the square is\", square.remote(42))"
   ]
